<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.kontak.KontakFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/relativ3"
            android:layout_below="@+id/relativ2"
            android:layout_marginTop="30dp"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Contact"
                android:textSize="20dp"
                android:textAlignment="center"
                android:id="@+id/title1"
                />

            <ImageView
                android:id="@+id/icon1"
                android:layout_width="27dp"
                android:layout_height="32dp"
                android:layout_below="@id/title1"
                android:layout_marginLeft="20dp"
                android:src="@drawable/telepon" />

            <TextView
                android:id="@+id/email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/title1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/icon1"
                android:text="082118301993"
                android:textSize="10dp" />

            <ImageView
                android:id="@+id/icon2"
                android:layout_width="31dp"
                android:layout_height="29dp"
                android:layout_below="@+id/icon1"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="7dp"
                android:src="@drawable/email" />

            <TextView
                android:id="@+id/instagram"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/email"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="23dp"
                android:layout_toRightOf="@id/icon2"
                android:text="yabiyabi127@gmail.com"
                android:textSize="10dp" />

            <ImageView
                android:id="@+id/icon3"
                android:layout_width="30dp"
                android:layout_height="31dp"
                android:layout_below="@id/icon2"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="6dp"
                android:src="@drawable/instagram" />

            <TextView
                android:id="@+id/whatsapp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/instagram"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="20dp"
                android:layout_toRightOf="@id/icon3"
                android:text="yabiyabi127"
                android:textSize="10dp" />
        </RelativeLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>